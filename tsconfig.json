{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-native",
    "lib": [
      "es6",
      "ES2021.String",
      "es2016",
      "ES2018",
      "ES2019"
    ],
    "module": "commonjs",
    "target": "es6",
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noImplicitUseStrict": false,
    "noStrictGenericChecks": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "incremental": true,
    "baseUrl": "./src/",
    "paths": {
      "utils/": [
        "utils/"
      ],
      "constants/*": [
        "constants/*"
      ],
      "helper/*": [
        "helper/*"
      ],
      "common": [
        "common/"
      ],
      "utils/*": [
        "utils/*"
      ],
      "routes/*": [
        "routes/*"
      ],
      "helpers/*": [
        "helpers/*"
      ],
      "assets": [
        "assets/"
      ],
      "screens": [
        "screens/"
      ],
      "components": [
        "components/"
      ],
      "core": [
        "core/"
      ],
      "service/*": [
        "service/*"
      ],
      "models": [
        "models/"
      ],
      "hooks": [
        "hooks/"
      ],
      "hoc": [
        "hoc/"
      ],
    },
    "resolveJsonModule": true,
    "noImplicitAny": true
  },
  "typedocOptions": {
    "entryPoints": [
      "src/App.tsx"
    ],
    "excludeInternal": true,
    "excludePrivate": true,
    "excludeProtected": true,
    "excludeExternals": true,
    "includeVersion": true,
    "out": "docs",
    "theme": "default"
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ]
}